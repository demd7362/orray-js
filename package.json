    "clean": "rmdir /s /q dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "author": "demd7362 <demd7362@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
